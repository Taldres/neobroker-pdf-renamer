{
  "name": "taldres/traderepublic-pdf-renamer",
  "description": "A tool to rename and optionally sort Trade Republic pdf files.",
  "authors": [
    {
      "name": "Taldres",
      "email": "contact@taldres.dev",
      "homepage": "https://github.com/Taldres",
      "role": "Developer"
    }
  ],
  "license": [
    "MIT"
  ],
  "keywords": [
    "traderepublic",
    "pdf renamer"
  ],
  "config": {
    "platform": {
      "php": "8.1"
    }
  },
  "bin": [
    "bin/console"
  ],
  "require": {
    "php": "^8.1",
    "symfony/console": "^6.2",
    "symfony/dependency-injection": "^6.2",
    "symfony/config": "^6.2",
    "symfony/yaml": "^6.2",
    "smalot/pdfparser": "^2.4",
    "ext-fileinfo": "*",
    "league/flysystem": "^3.12",
    "league/mime-type-detection": "^1.11",
    "symfony/event-dispatcher": "^6.2",
    "symfony/translation": "^6.2",
    "symfony/dotenv": "^6.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.15"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src",
      "Vendor\\": "vendor"
    }
  },
  "scripts": {
    "csfixer": [
      "vendor/bin/php-cs-fixer fix src --rules=@PSR12"
    ],
    "phpstan": [
      "phpstan analyze -c phpstan.neon"
    ]
  }
}
